<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.honeste.honest_e">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
        tools:replace="android:icon,android:theme">
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeUI" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".NewComplaint" />
        <activity android:name=".MainActivity" />
        <activity android:name=".comment_list" />
        <activity
            android:name=".Edit_Complaint"
            android:noHistory="true" />
        <activity
            android:name=".Edit_Comment"
            android:noHistory="true" />
        <activity android:name=".InflaterHome" />
        <activity android:name=".AMC_Login"></activity>
    </application>

</manifest>